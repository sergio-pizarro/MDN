    <script type="text/javascript">
        var CargadorTotales = {

            CargaTrakingPaisTotales: function (p_periodo) {
                //Pre Aprobados
                $.SecGetJSON(BASE_URL + "/motor/api/Informes/lista-traking-total-pais", { periodo: p_periodo }, function (menus) {

                    $("#idTotalAsignados").html(menus.SumAsignado.toMoney());
                    $("#idTotalGestionados").text(menus.SumGestionado.toMoney());
                    $("#IdDiaHAbil").text(menus.DiaHabil);
                    $("#idFechaActualizacion").text(menus.FechaActualizacion);
                    $("#idSucursal").text(menus.Pais);
                    $("#idPorcentajeGestionados2").text((menus.PorcentajeGestionado).toMoney(0) + '%');
                    $("#idPorcentajeGestionados1").text((menus.PorcentajeGestionado).toMoney(0) + '%');
                    $("#idTotalContactados").text(menus.SumContactado.toMoney());
                    $("#idPorcentajeContactados").text((menus.PorcentajeContactado).toMoney(0) + '%');
                    $("#idTotalInteresados").text(menus.SumInteresado.toMoney());
                    $("#idPorcentajeInteresados").text((menus.PorcentajeInteresado).toMoney(0) + '%');
                    $("#idTotalPresentados").text(menus.SumPresentado.toMoney());
                    $("#idPorcentajePresentados").text((menus.PorcentajePresentado).toMoney(0) + '%');
                    $("#idTotalAprobados").text(menus.SumAprobado.toMoney());
                    $("#idPorcentajeAprobados").text((menus.PorcentajeAprobado).toMoney(0) + '%');
                    $("#idTotalCursados").text(menus.SumCursado.toMoney());
                    $("#idPorcentajeCursados").text((menus.PorcentajeCursado).toMoney(0) + '%');
                    $("#idPromedioNeto").text(menus.PromNeto.toMoney());

                    // <label id=""></label> - <label id=""></label>
                    //.text(menus.SumaAsignadosWid);

                });
                $.SecGetJSON(BASE_URL + "/motor/api/Informes/lista-traking-combobox", { periodo: p_periodo }, function (datos) {
                    $("#demo-foo-filter-status").html("");
                    $("#demo-foo-filter-status").append($("<option>").attr("value", "").html("Todos"));
                    $.each(datos, function (i, e) {
                        $("#demo-foo-filter-status").append($("<option>").attr("value", e.EjecutivoCargo).html(e.EjecutivoCargo))
                    });
                });

                var filtering = $('#demo-foo-filtering');
                filtering.footable().on('footable_filtering', function (e) {
                var selected = $('#demo-foo-filter-status').find(':selected').html();

                    if (selected !== "Todos") {
                        e.filter += (e.filter && e.filter.length > 0) ? ' ' + selected : selected;

                    }
                    e.clear = !e.filter;
                });

                $('#demo-foo-filter-status').change(function (e) {
                    e.preventDefault();
                    filtering.trigger('footable_filter', { filter: '' });

                });


            },
            CargaTrakingZonal: function (p_periodo) {
                //Pre Aprobados
                $("#bdy_datos").html("");
                $.SecGetJSON(BASE_URL + "/motor/api/Informes/lista-traking-total-zonal", { periodo: p_periodo }, function (menus) {
                    $.each(menus, function (i, e) {
                        $("#bdy_datos")
                            .append(
                                $("<tr>")
                                    .append($("<td>").append('<a href="/motor/App/Informes/TrackingZonal?Periodo=' + p_periodo + '&CodZona=' + e.CodZona + '&tab=' + $("#PrincipalTabActivo").val() + '" class="btn-link">' + e.Zona + '</a>')) //link
                                    .append($("<td>").append(e.SumAsignado.toMoney(0)))
                                    .append($("<td>").append(e.SumGestionado.toMoney(0)))
                                    .append($("<td>").append(e.PorcentajeGestionado.toMoney(0) + '%'))
                                    .append($("<td>").append(e.SumContactado.toMoney(0)))
                                    .append($("<td>").append(e.PorcentajeContactado.toMoney(0) + '%'))
                                    .append($("<td>").append(e.SumInteresado.toMoney(0)))
                                    .append($("<td>").append(e.PorcentajeInteresado.toMoney(0) + '%'))
                                    .append($("<td>").append(e.SumPresentado.toMoney(0)))
                                    .append($("<td>").append(e.PorcentajePresentado.toMoney(0) + '%'))
                                    .append($("<td>").append(e.SumAprobado.toMoney(0)))
                                    .append($("<td>").append(e.PorcentajeAprobado.toMoney(0) + '%'))
                                    .append($("<td>").append(e.SumCursado.toMoney(0)))
                                    .append($("<td>").append(e.PorcentajeCursado.toMoney(0) + '%'))
                                    .append($("<td>").append(e.PromNeto.toMoney(0)))
                            );
                    });
                });
            },
           




        }
        //Recarga de periodo de preaprobados


        $.SecGetJSON(BASE_URL + "/motor/api/Informes/listar-periodos-tracking", function (datos) {

            $("#slPeriodo").html("");
            $.each(datos, function (i, periodo) {
                $("#slPeriodo").append($("<option>").val(periodo.Periodo).html(periodo.PeriodoText));
            });
        
            //Recarga de periodo de preaprobados
            $("#slPeriodo").on("change", function () {
                CargadorTotales.CargaTrakingPaisTotales($(this).val());
                CargadorTotales.CargaTrakingZonal($(this).val());
               
            });

            //Trigger para primera carga de preaprobaDOs
            $("#slPeriodo").trigger("change");
        });
