    <script type="text/javascript">
        var Cargador = {
            CargaTrakingSucursal: function (p_periodo) {
                //Pre Aprobados
                $("#bdy_datos").html("");
                $.SecGetJSON(BASE_URL + "/motor/api/Informes/lista-traking", { periodo: p_periodo }, function (menus) {
                    $.each(menus, function (i, e) {
                        $("#bdy_datos")
                            .append(
                                $("<tr>")
                                    .append($("<td>").append('<a href="/motor/App/Informes/DetalleEjecutivoTracking?RutEjecutivo=' + e.EjecAsign_Rut + '&Periodo=' + p_periodo + '&tab=' + $("#PrincipalTabActivo").val() + '" class="btn-link">' + e.EjecutivoNombre + '</a>')) //link
                                    .append($("<td>").append(e.EjecutivoCargo).hide())
                                    .append($("<td>").append(e.SumaAsignados.toMoney(0)))
                                    .append($("<td>").append(e.SumaGestionados.toMoney(0)))
                                    .append($("<td>").append(e.SumaPorcentajeGestionado.toMoney(0)+'%'))
                                    .append($("<td>").append(e.SumaContactados.toMoney(0)))
                                    .append($("<td>").append(e.SumaPorcentajeContactados.toMoney(0) + '%'))
                                    .append($("<td>").append(e.SumaInteresados.toMoney(0)))
                                    .append($("<td>").append(e.SumaPorcentajeInteresados.toMoney(0) + '%'))
                                    .append($("<td>").append(e.SumaPresentados.toMoney(0)))
                                    .append($("<td>").append(e.SumaPorcentajePresentados.toMoney(0) + '%'))
                                    .append($("<td>").append(e.SumaAprobados.toMoney(0)))
                                    .append($("<td>").append(e.SumaPorcentajeAprobados.toMoney(0) + '%'))
                                    .append($("<td>").append(e.SumaCursados.toMoney(0)))
                                    .append($("<td>").append(e.SumaPorcentajeCursados.toMoney(0) + '%'))
                                    .append($("<td>").append(e.MontoPromedio.toMoney(0)))
                            );
                    });
                    //////////////////////////////////////////////////////////////////////////////////
                    // Filtros
                    var filtering = $('#demo-foo-filtering');
                    filtering.footable().on('footable_filtering', function (e) {
                    });
                    $('#demo-foo-search').on('input', function (e) {
                        e.preventDefault();
                        if ($(this).val().length >= 5 || $(this).val().length == 0) {
                            filtering.trigger('footable_filter', { filter: $(this).val() });
                        }
                    });
                });
            },

        }
        var CargadorTotales = {
            CargaTrakingSucursalTotales: function (p_periodo) {
                //Pre Aprobados
                $.SecGetJSON(BASE_URL + "/motor/api/Informes/lista-traking-total", { periodo: p_periodo }, function (menus) {
                    
                    $("#idTotalAsignados").html(menus.SumaAsignadosWid.toMoney());
                    $("#idTotalGestionados").text(menus.SumaGestionadosWid);
                    $("#IdDiaHAbil").text(menus.diaHabilActual);
                    $("#idFechaActualizacion").text(menus.Actualizacion);
                    $("#idSucursal").text(menus.Oficina);
                    $("#idPorcentajeGestionados2").text((menus.SumaPorcentajeGestionadoWid).toMoney(0) + '%');
                    $("#idPorcentajeGestionados1").text((menus.SumaPorcentajeGestionadoWid).toMoney(0) + '%');
                    $("#idTotalContactados").text(menus.SumaContactadosWid);
                    $("#idPorcentajeContactados").text((menus.SumaPorcentajeContactadosWid).toMoney(0) + '%');
                    $("#idTotalPresentados").text(menus.SumaPresentadosWid);
                    $("#idPorcentajePresentados").text((menus.SumaPorcentajePresentadosWid).toMoney(0) + '%');
                    $("#idTotalAprobados").text(menus.SumaAprobadosWid);
                    $("#idPorcentajeAprobados").text((menus.SumaPorcentajeAprobadosWid).toMoney(0) + '%');
                    $("#idTotalCursados").text(menus.SumaCursadosWid);
                    $("#idPorcentajeCursados").text((menus.SumaPorcentajeCursadosWid).toMoney(0) + '%');
                    $("#idPromedioNeto").text(menus.MontoNetoPromWid.toMoney());

                    // <label id=""></label> - <label id=""></label>
                    //.text(menus.SumaAsignadosWid);




                });
                $.SecGetJSON(BASE_URL + "/motor/api/Informes/lista-traking-combobox", { periodo: p_periodo }, function (datos) {
                    $("#demo-foo-filter-status").html("");
                    $("#demo-foo-filter-status").append($("<option>").attr("value", "").html("Todos"));
                    $.each(datos, function (i, e) {
                        $("#demo-foo-filter-status").append($("<option>").attr("value", e.EjecutivoCargo).html(e.EjecutivoCargo))
                    });
                });

                var filtering = $('#demo-foo-filtering');
                filtering.footable().on('footable_filtering', function (e) {
                var selected = $('#demo-foo-filter-status').find(':selected').html();

                    if (selected !== "Todos") {
                        e.filter += (e.filter && e.filter.length > 0) ? ' ' + selected : selected;

                    }
                    e.clear = !e.filter;
                });

                $('#demo-foo-filter-status').change(function (e) {
                    e.preventDefault();
                    filtering.trigger('footable_filter', { filter: '' });
                    
                });


            },

        }
        //Recarga de periodo de preaprobados
        $.SecGetJSON(BASE_URL + "/motor/api/Informes/listar-periodos-tracking", function (datos) {

            $("#slPeriodo").html("");
            $.each(datos, function (i, periodo) {
                $("#slPeriodo").append($("<option>").val(periodo.Periodo).html(periodo.PeriodoText));
            });

            //Recarga de periodo de preaprobados
            $("#slPeriodo").on("change", function () {
                Cargador.CargaTrakingSucursal($(this).val());
                CargadorTotales.CargaTrakingSucursalTotales($(this).val());
            });

            //Trigger para primera carga de preaprobaDOs
            $("#slPeriodo").trigger("change");
        });
        //normalizacion
        var CargadorNormalizacion = {
            CargaTrakingSucursalNormalizacion: function (p_periodo) {
                //Pre Aprobados
                $("#bdy_datos_normalizacion").html("");
                $.SecGetJSON(BASE_URL + "/motor/api/Informes/Lista-Traking-Normalizacion", { periodo: p_periodo }, function (menus) {
                    $.each(menus, function (i, e) {
