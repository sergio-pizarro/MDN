                //Pre Aprobados
                $.SecGetJSON(BASE_URL + "/motor/api/Informes/lista-traking-total-Normalizacion", { periodo: p_periodo }, function (menus) {

                    $("#idTotalAsignados_Normalizacion").html(menus.SumaAsignadosWid.toMoney());
                    $("#idTotalGestionados_Normalizacion").text(menus.SumaGestionadosWid);
                    $("#IdDiaHAbil_Normalizacion").text(menus.diaHabilActual);
                    $("#idFechaActualizacion_Normalizacion").text(menus.Actualizacion);
                    $("#idSucursal_Normalizacion").text(menus.Oficina);
                    $("#idPorcentajeGestionados2_Normalizacion").text((menus.SumaPorcentajeGestionadoWid).toMoney(0) + '%');
                    $("#idPorcentajeGestionados1_Normalizacion").text((menus.SumaPorcentajeGestionadoWid).toMoney(0) + '%');
                    $("#idTotalContactados_Normalizacion").text(menus.SumaContactadosWid);
                    $("#idPorcentajeContactados_Normalizacion").text((menus.SumaPorcentajeContactadosWid).toMoney(0) + '%');
                    $("#idTotalPresentados_Normalizacion").text(menus.SumaPresentadosWid);
                    $("#idPorcentajePresentados_Normalizacion").text((menus.SumaPorcentajePresentadosWid).toMoney(0) + '%');
                    $("#idTotalAprobados_Normalizacion").text(menus.SumaAprobadosWid);
                    $("#idPorcentajeAprobados_Normalizacion").text((menus.SumaPorcentajeAprobadosWid).toMoney(0) + '%');
                    $("#idTotalCursados_Normalizacion").text(menus.SumaCursadosWid);
                    $("#idPorcentajeCursados_Normalizacion").text((menus.SumaPorcentajeCursadosWid).toMoney(0) + '%');
                    $("#idPromedioNeto_Normalizacion").text(menus.MontoBrutoPromWid.toMoney());

                    // <label id=""></label> - <label id=""></label>
                    //.text(menus.SumaAsignadosWid);

                });
                $.SecGetJSON(BASE_URL + "/motor/api/Informes/lista-traking-combobox-normalizacion", { periodo: p_periodo }, function (datos) {
                    $("#demo-foo-filter-status-Normalizacion").html("");
                    $("#demo-foo-filter-status-Normalizacion").append($("<option>").attr("value", "").html("Todos"));
                    $.each(datos, function (i, e) {
                        $("#demo-foo-filter-status-Normalizacion").append($("<option>").attr("value", e.EjecutivoCargo).html(e.EjecutivoCargo))
                    });
                });

                var filtering = $('#demo-foo-filtering-Normalizacion');
                filtering.footable().on('footable_filtering', function (e) {
                    var selected = $('#demo-foo-filter-status-Normalizacion').find(':selected').html();

                    if (selected !== "Todos") {
                        e.filter += (e.filter && e.filter.length > 0) ? ' ' + selected : selected;

                    }
                    e.clear = !e.filter;
                });

                $('#demo-foo-filter-status-Normalizacion').change(function (e) {
                    e.preventDefault();
                    filtering.trigger('footable_filter', { filter: '' });

                });


            },

        }
        //Recarga de periodo de preaprobados
        $.SecGetJSON(BASE_URL + "/motor/api/Informes/listar-periodos-tracking", function (datos) {

            $("#slPeriodo_Normalizacion").html("");
            $.each(datos, function (i, periodo) {
                $("#slPeriodo_Normalizacion").append($("<option>").val(periodo.Periodo).html(periodo.PeriodoText));
