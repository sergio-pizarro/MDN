@{
    ViewBag.Title = "CargasFamiliares";
}



@{
    Layout = "~/Areas/AppPage/Views/Shared/_Layout.cshtml";
}
<!--Masked Input [ OPTIONAL ]-->
<script src="~/Assets/plugins/masked-input/jquery.maskedinput.min.js"></script>
<!--Magic Checkbox [ OPTIONAL ]-->
<link href="~/Assets/plugins/magic-check/css/magic-check.min.css" rel="stylesheet">
<link href="~/Assets/plugins/bootstrap-validator/bootstrapValidator.min.css" rel="stylesheet">
<!--Bootstrap Select [ OPTIONAL ]-->
<link href="~/Assets/plugins/bootstrap-select/bootstrap-select.min.css" rel="stylesheet">
<script src="~/Assets/plugins/bootbox/bootbox.min.js"></script>

<!--Page content-->
<!--===================================================-->
<div id="page-content">
    <div class="row">
        <div class="col-lg-12">
            <div class="tab-base">

                <div class="tab-content">
                    <h4> CARGAS FAMILIARES</h4>
                    <div class="panel-body">
                        <div class="pad-btm">
                            <div class="row">
                                <div class="col-sm-2 text-xs-center" style="display:none">
                                    <div class="form-group">
                                        <label class="control-label">Estado</label>
                                        <select id="demo-foo-filter-status" class="form-control">
                                            <option value="">Todos</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-2 text-xs-center" style="display:none">
                                    <div class="form-group">
                                        <label class="control-label">Sub Estado</label>
                                        <select id="demo-foo-filter-statusSub" class="form-control">
                                            <option value="">Todos</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-1 text-xs-center" style="width: 130px;display:none">
                                    <div class="form-group">
                                        <label class="control-label">Marcas</label>
                                        <select id="demo-foo-filter-statusMarca" class="form-control">
                                            <option value="">Seleccione</option>
                                            <option value="Pensionados">Pensionados Nuevos</option>
                                            <option value="Cumpleanos">Cumpleaños</option>
                                            <option value="Cartera">Compra Cartera</option>
                                            <option value="BNF">Beneficios</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-1 text-xs-center" style="display:none">
                                    <div class="form-group">
                                        <label class="control-label">Prioridad</label>
                                        <select id="slPrioridad" class="form-control selectpicker">
                                            <option value="">Todos</option>
                                            <option data-content='<span class="badge badge-success">1</span>'>1</option>
                                            <option data-content='<span class="badge badge-yelow">2</span>'>2</option>
                                            <option data-content='<span class="badge badge-warning">3</span>'>3</option>
                                            <option data-content='<span class="badge badge-danger">4</span>'>4</option>
                                            <option data-content='<span class="badge badge-purple">5</span>'>5</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-1 text-xs-center" style="display:none">
                                    <div class="form-group">
                                        <label class="control-label">Segmento</label>
                                        <select id="slSegmento" class="form-control">
                                            <option value="">Todos</option>
                                            <option value="Pensionados">Pensionados</option>
                                            <option value="Privados">Privados</option>
                                            <option value="Publicos">Públicos</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-2 text-xs-center" style="display:none">
                                    <div class="form-group">
                                        <label class="control-label">Tipo</label>
                                        <select id="slTipo" class="form-control">
                                            <option value="">Todos</option>
                                            <option value="NUEVOS">Nuevos</option>
                                            <option value="PARALELO">Paralelo</option>
                                            <option value="RENEGOCIACION">Renegociación</option>
                                            <option value="PENSIONADOS TARGET">Pensionados Target</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-2 text-xs-center" style="display:none">
                                    <div class="form-group">
                                        <label class="control-label">Vencimiento</label>
                                        <select id="flt_vencidos" class="form-control">
                                            <option value="">Todos</option>
                                            <option value="AYER">Vencidos</option>
                                            <option value="HOY">Vence Hoy</option>
                                            <option value="PROX5DIAS">Prox. 5 días</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-2 text-xs-center" style="width: 358px;display:none">
                                    <div class="form-group">
                                        <label class="control-label">Busqueda por Empresa</label>
                                        <select id="demo-chosen-select" tabindex="2"></select>
                                    </div>
                                </div>
                                <div class="col-sm-2 text-xs-center" style="display:none">
                                    <label class="control-label">Rut</label>
                                    <div class="input-group mar-btm">
                                        <input id="demo-foo-search" type="text" placeholder="Ej. 11.111.111-1" class="form-control" autocomplete="off">
                                        <input id="filtrar" type="text" class="form-control" placeholder="Buscar...">
                                        <div class="input-group-btn">
                                            <button data-target="#demo-lg-modal-search" data-toggle="modal" class="btn btn-primary" type="button">
                                                <i class="pli-search-on-cloud"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-1">
                                    <button id="button" class="btn btn-default">Buscar Afiliados</button>
                                </div>
                                <div class="col-sm-6" style="display:none">
                                    <h4>Para visualizar datos debe presionar el botón filtrar.</h4>
                                </div>
                            </div>
                        </div>
                        <div class="pad-btm">
                            <div class="row">

                                <div class="col-sm-3 ">
                                    <div class="form-group">
                                        <label class="control-label">Ingrese dato a buscar</label>
                                        <input id="flt_general" type="text" placeholder="Palabra Clave" class="form-control" autocomplete="off">
                                    </div>

                                </div>
                            </div>
                        </div>
                        <table id="demo-foo-filtering" class="table table-bordered table-hover toggle-circle" data-page-size="30" style="font-size: 11px;">
                            <thead>
                                <tr>
                                    <th data-toggle="true">Rut Afiliado</th>
                                    <th data-toggle="true">Nombre Afiliado</th>
                                    <th data-hide="phone">Apellido Afiliado</th>
                                    <th data-hide="phone">Cant.Cargas</th>
                                    <th data-hide="phone">Rut Empresa</th>
                                    <th data-hide="phone">Nombre Empresa</th>
                                    <th data-hide="phone">Estado</th>
                                </tr>
                            </thead>

                            <tbody id="bdy_datos"></tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="10">
                                        <div class="text-right">
                                            <ul class="pagination"></ul>
                                        </div>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>

                    </div>


                </div>
            </div>
        </div>


    </div>
</div>

<!--Default Bootstrap Modal-->
<!--===================================================-->
<div id="modal-cargas" class="modal fade" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><i class="pci-cross pci-circle"></i></button>
                <h4 class="modal-title" id="myLargeModalLabel">Gestión</h4>
            </div>


            <!--Modal body-->
            <div class="modal-body">
                <div class="tab-base" id="MDL_Primario" style="margin-bottom: 0px !important;">
                    <!--Nav Tabs-->
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a data-toggle="tab" href="#tab-gestion-1" id="tab_contacti" aria-expanded="true">Cargas</a>
                        </li>

                        <li class="">
                            <a data-toggle="tab" href="#tab-gestion-3" aria-expanded="false">Contactabilidad</a>
                        </li>

                    </ul>

                </div>

                <div class="tab-content" style="box-shadow: 0 0 0 !important;">
                    <div id="tab-gestion-1" class="tab-pane fade active in">
                        <br />
                        <div class="row">

                            <div class="col-sm-3 text-xs-center" style=" color:black;">
                                <div class="form-group">
                                    <label class="control-label" ><b>RUT AFILIADO:</b> </label>
                                    <label class="control-label" id="lblrutAfiliado"></label>
                                </div>
                            </div>
                            <div class="col-sm-5 text-xs-center" style=" color:black;">
                                <div class="form-group">
                                    <label class="control-label"><b>AFILIADO:</b> </label>
                                    <label class="control-label" id="lblafiliado"></label>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-3 text-xs-center" style=" color:black;">
                                <div class="form-group">
                                    <label class="control-label"><b>RUT EMPRESA:</b> </label>
                                    <label class="control-label" id="lblrutempresa"></label>
                                </div>
                            </div>
                            <div class="col-sm-5 text-xs-center">
                                <div class="form-group" style=" color:black;">
                                    <label class="control-label"><b>EMPRESA:</b> </label>
                                    <label class="control-label" id="lblempresa"></label>
                                </div>
                            </div>

                                               </div>

                        <table id="demo-foo-filtering-cargas" data-page-size="20" class="table table-bordered table-hover toggle-circle" style="font-size: 11px;">
                            <thead>
                                <tr>
                                    @*<th data-toggle="true">Rut Afil.</th>
        <th data-toggle="true">Afiliado</th>
        <th data-toggle="true">Rut Empresa</th>
        <th data-toggle="true">Empresa</th>*@
                                    <th data-toggle="true">Rut Carga</th>
                                    <th data-toggle="true">Nombre Carga</th>
                                    <th data-toggle="true">Apellido Carga</th>
                                    <th data-toggle="true">Fecha Nac.</th>
                                    <th data-toggle="true">Cod.Caus.</th>
                                    <th data-toggle="true">Estado</th>
                                    <th data-toggle="true">Editar</th>
                                </tr>
                            </thead>

                            <tbody id="bdy_datos_busqueda_cargas"></tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="10">
                                        <div class="text-right">
                                            <ul class="pagination"></ul>
                                        </div>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>

                    </div>
                    <div id="tab-gestion-3" class="tab-pane fade">
                        <div class="row">
                            <br />

                            <button type="button" id="btn-add-contac" class="btn btn-primary btn-xs">+</button>
                            <br />
                            <br />
                        </div>
                        <div class="mar-btm" style="display: none;" id="formulario-contac">
                            <form id="form-registro-contacto" action="#" method="post">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <div class="btn-group bootstrap-select">
                                                <label class="control-label">Tipo de Contacto</label>
                                                <select class="selectpicker" id="cbtippContac" name="cbtippContac">
                                                    <option value="">Seleccione..</option>
                                                    <option value="1">Celular</option>
                                                    <option value="2">Teléfono</option>
                                                    <option value="3">Mail</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <div class="btn-group bootstrap-select">
                                                <label class="control-label">Clasificación de Contacto</label>
                                                <select class="selectpicker" id="cbClasificacionConctac" name="cbClasificacionConctac">
                                                    <option value="">Seleccione..</option>
                                                    <option value="1">Personal</option>
                                                    <option value="2">Comercial</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label class="control-label">Datos de Contacto</label>
                                            <input class="form-control" id="afi_NewContacto" type="text" value="" maxlength="150" name="afi_NewContacto">
                                        </div>
                                    </div>
                                </div>
                                <button class="btn btn-primary" id="btGuardaContacto" type="submit">Guardar Contacto</button>
                            </form>
                        </div>
                        <div class="row">

                            <table class="table table-hover table-vcenter" style="font-size: 11px;">
                                <thead>
                                    <tr>
                                        <th>Gestión</th>
                                        <th>Datos Contacto</th>
                                        <th>Tipo contacto</th>
                                        <th>Indice</th>
                                        <th>Ultimo Contacto Valido</th>
                                    </tr>
                                </thead>
                                <tbody id="bdy_datos_contactos"></tbody>
                            </table>
                        </div>
                        <div class="modal-footer" id="mensahes-must" style="display:none;">
                            <form id="datos-gestion" action="#" method="post" style="display:none;">
                                <div class="row esconder" style="text-align: left;">
                                    <div class="col-sm-4">
                                        <div class="form-group">
                                            <label class="control-label">Estado</label>
                                            <select class="form-control" id="ges_estado" name="ges_estado">
                                                <option value="">Seleccione</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group">
                                            <label class="control-label">Sub Estado</label>
                                            <select class="form-control" id="ges_subestado" name="ges_subestado" disabled="disabled">
                                                <option value="">Seleccione</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group" id="fpg">
                                            <label class="control-label">Fecha Próxima Gestión</label>
                                            <div id="demo-dp-component">
                                                <div class="input-group date">
                                                    <input type="text" class="form-control" id="ges_prox_gestion" name="ges_prox_gestion">
                                                    <span class="input-group-addon"><i class="demo-pli-calendar-4"></i></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row esconder" style="text-align: left;">
                                    <div class="col-sm-12">
                                        <div class="form-group">
                                            <label class="control-label">Comentarios</label>
                                            <textarea placeholder="Comentarios" rows="3" class="form-control" id="ges_comentarios" name="ges_comentarios"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <input type="hidden" id="ges_id_asignacion" name="ges_id_asignacion" value="" />
                                <button data-dismiss="modal" class="btn btn-default" type="button">Cerrar</button>
                                <button class="btn btn-primary esconder" id="btn_savear" type="submit">Guardar</button>
                            </form>

                        </div>
                    </div>
                   







                </div>
            </div>

         
            <!--===================================================-->
            <input type="hidden" id="hdRutEjec" />
            <input type="hidden" id="hdIdAusencia" value="0" />
            <div id="dvMensajes"></div>
            <!--Modal footer-->
           <div class="modal-footer">
                <button data-dismiss="modal" class="btn btn-default" type="button">Cerrar</button>
                @*<button class="btn btn-primary" id="bt-modificar-ausencia" style="display:none;" type="button">Modificar</button>
                <button class="btn btn-primary" id="bt-guardar-ausencia" type="submit">Guardar</button>*@
            </div>

        </div>
      

    </div>
</div>

<!--===================================================-->
<!--End Default Bootstrap Modal-->



@section script{
    <script src="~/Assets/plugins/fooTable/dist/footable.all.min.js"></script>
    <script src="~/Assets/plugins/bootstrap-validator/bootstrapValidator.min.js"></script>
    <!--Bootstrap Select [ OPTIONAL ]-->
    <script src="~/Assets/plugins/bootstrap-select/bootstrap-select.min.js"></script>


    <script src="~/Assets/js.App/src/CargasFamiliares.js?g=@Guid.NewGuid().ToString()"></script>

}